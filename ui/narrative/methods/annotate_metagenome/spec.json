{
    "ver" : "0.0.1",
    "name" : "Annotate a metagenome",
    "authors" : [ "chenry", "qzhang" ],
    "contact" : "help@kbase.us",
    "categories" : ["active","annotation"],
    "widgets" : {
        "input" : "kbaseNarrativeMethodInput",
        "output" : "kbaseGenomeView"
    },
    "parameters" : [
        {
            "id" : "object_ref",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
            "valid_ws_types" : [ "KBaseGenomeAnnotations.Assembly", "KBaseMetagenomes.AnnotatedMetagenomeAssembly"]
            }
        }, {
            "id" : "output_metagenome_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
            "valid_ws_types" : [ "KBaseMetagenomes.AnnotatedMetagenomeAssembly" ],
            "is_output_name":true
           }
        }, {
            "id" : "run_prodigal",
            "optional":false,
            "advanced":false,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 0,
                "unchecked_value": 1
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "RAST_SDK",
            "method": "annotate_metagenome",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "output_workspace"
                },
                {
                    "input_parameter": "object_ref",
                    "target_property": "object_ref",
                    "target_type_transform": "ref"
                },
		{
		    "input_parameter": "output_metagenome_name",
		    "generated_value": {
		        "prefix": "metagenome_"
		    },
		    "target_property": "output_metagenome_name"
		},
                {
                    "input_parameter": "run_prodigal",
                    "target_property": "run_prodigal"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "input_parameter": "output_metagenome_name",
                    "target_property": "id"
                },
                {
                    "input_parameter": "run_prodigal",
                    "target_property": "run_prodigal"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "output_workspace"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
